{
  "background.service_worker": {
    "persistent": false,
    "scripts": [
      "./js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/js/dictionary_scripts.js",
        "/js/registration.js"
      ]
    }
  ],
  "action": {
    "default_icon": "/img/icon.png",
    "default_popup": "/html/index.html",
    "default_title": "Saver",
    "icons": {
      "128":"/img/icon.png",
      "16": "/img/icon.png",
      "32": "/img/icon.png",
      "48": "/img/icon.png"
    }
  },

  "manifest_version": 3,
  "name": "Saver",
  "permissions": [
    "activeTab",
    "cookies",
    "clipboardWrite",
    "storage",
    "background",
    "tabs",
    "scripting"
  ],
  "short_name": "Saver",
  "version": "1.1"
}